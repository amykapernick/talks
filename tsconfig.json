{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"plugins": [
			{
			"name": "@astrojs/ts-plugin"
			}
		],
		"types": [
			"node",
			"@astrojs/image/client",
			"astro/client"
		],
		"paths": {
			"@components/*": [
			"src/components/*.astro"
			],
			"@ts/*": [
			"src/types/*"
			],
			"@fonts/*": [
			"src/fonts/*"
			],
			"@partials/*": [
			"src/partials/*.astro"
			],
			"@img/*": [
			"src/img/*"
			],
			"@pages/*": [
			"src/pages/*.astro"
			],
			"@styles/*": [
			"src/styles/*",
			],
			"@utils/*": [
			"utils/*"
			],
			"@layouts/*": [
			"src/layouts/*.astro"
			],
			"@data/*": [
			"_data/*"
			],
			"@react/*": [
			"src/react/*"
			]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
  }