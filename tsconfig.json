{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"plugins": [
			{
			"name": "@astrojs/ts-plugin"
			}
		],
		"types": [
			"node",
			"@astrojs/image/client",
			"astro/client"
		],
		"paths": {
			"@slides/*": [
				"src/slides/*.astro"
			],
			"@ts/*": [
				"src/types/*"
			],
			"@parts/*": [
				"src/parts/*.astro"
			],
			"@img/*": [
				"src/img/*"
			],
			"@pages/*": [
				"src/pages/*.astro"
			],
			"@styles/*": [
				"src/styles/*",
			],
			"@utils/*": [
				"utils/*"
			]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
  }